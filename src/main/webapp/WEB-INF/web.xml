<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app

        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns="http://java.sun.com/xml/ns/javaee"
        xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
        xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
        id="Your_WebApp_ID"
        version="2.5">


  <servlet>
    <servlet-name>ShowAll</servlet-name>
    <servlet-class>controller.ShowAll</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>AddEditUser</servlet-name>
    <servlet-class>controller.AddEditUser</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>AddEditProduct</servlet-name>
    <servlet-class>controller.AddEditProduct</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>ShowAll</servlet-name>
    <url-pattern>/ShowAll</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>AddEditUser</servlet-name>
    <url-pattern>/AddEditUser</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>AddEditProduct</servlet-name>
    <url-pattern>/AddEditProduct</url-pattern>
  </servlet-mapping>


  <display-name>Archetype Created Web Application</display-name>
</web-app>
